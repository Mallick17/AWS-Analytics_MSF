# ==================================================================================
# Environment Configuration for PyFlink + S3 Tables
# ==================================================================================
# Copy this file to .env and fill in your actual values
# ==================================================================================

# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=ap-south-1

# S3 Tables Configuration (use ARN format for S3 Tables buckets)
# Format: arn:aws:s3tables:<region>:<account-id>:bucket/<bucket-name>
S3_WAREHOUSE=arn:aws:s3tables:ap-south-1:123456789012:bucket/your-s3tables-bucket

# Flink/Iceberg Configuration
FLINK_ENV=local
ICEBERG_NAMESPACE=analytics
