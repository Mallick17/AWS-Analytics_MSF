# AWS Credentials
AWS_ACCESS_KEY_ID=AKIA...
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=ap-south-1

# S3 Tables Configuration (use ARN format for S3 Tables, not standard S3)
# Format: arn:aws:s3tables:<region>:<account-id>:bucket/<bucket-name>
S3_WAREHOUSE=arn:aws:s3tables:ap-south-1:123456789012:bucket/your-s3tables-bucket

# Standard S3 Bucket for Flink checkpoints/savepoints and local REST catalog
S3_WAREHOUSE_BUCKET=s3://your-standard-s3-bucket

# Flink/Iceberg Configuration
FLINK_ENV=production
ICEBERG_NAMESPACE=analytics
